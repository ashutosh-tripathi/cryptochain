"column":19},"source":"components/ConductTransaction.js","original":{"line":35,"column":12}},{"generated":{"line":124,"column":20},"source":"components/ConductTransaction.js","original":{"line":34,"column":8}},{"generated":{"line":125,"column":0},"source":"components/ConductTransaction.js","original":{"line":67,"column":5}},{"name":"Component","generated":{"line":129,"column":2},"source":"components/ConductTransaction.js","original":{"line":5,"column":33}},{"name":"ConductTransaction","generated":{"line":131,"column":15},"source":"components/ConductTransaction.js","original":{"line":69,"column":15}}],"sources":{"components/ConductTransaction.js":"import React,{Component} from 'react'\nimport {FormGroup,FormControl,Button} from 'react-bootstrap'\nimport {Link} from 'react-router-dom'\n\nclass ConductTransaction extends Component{\n    state={recipient:'',amount:0}\n    updateRecipient = event => {\n        this.setState({ recipient: event.target.value });\n      }\n    \n      updateAmount = event => {\n        this.setState({ amount: Number(event.target.value) });\n      }\n      conductTransaction=()=>{\n          const {recipient,amount}=this.state\n          fetch(`${document.location.origin}/api/transact`,{\n              method: 'POST',\n              headers:{'Content-Type':'application/json'},\n              body:JSON.stringify({recipient,amount})\n          }).then(response => response.json())\n          .then(json => {\n              console.log(json.transaction.id)\n              console.log(JSON.stringify(json))\n            alert(json.message || json.transaction.id);\n           \n          });\n            \n            \n      }\n\n\n    render(){\n        // console.log(\"state\",this.state)\n        return (\n            <div className=\"ConductTransaction\">\n                <Link to=\"/\">Home</Link>\n                <Link to=\"/blocks\">Blocks</Link>\n                <h3>Conduct a  transaction</h3>\n                <FormGroup>\n          <FormControl\n            input='text'\n            placeholder='recipient'\n            value={this.state.recipient}\n            onChange={this.updateRecipient}\n          />\n        </FormGroup>\n        <FormGroup>\n          <FormControl\n            input='number'\n            placeholder='amount'\n            value={this.state.amount}\n            onChange={this.updateAmount}\n          />\n        </FormGroup>\n            <div>\n            <Button\n            bsstyle=\"danger\"\n             bssize=\"small\"\n             onClick={this.conductTransaction}\n                >\n              Submit\n            </Button>\n               </div> \n\n            </div>\n        )\n    }\n}\nexport default ConductTransaction"},"lineCount":null}},"error":null,"hash":"54eedc99864f2ac3c7a5e5700c5a90a6","cacheData":{"env":{}}}