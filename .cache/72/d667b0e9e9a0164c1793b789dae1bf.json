,"original":{"line":46,"column":30}},{"generated":{"line":109,"column":0},"source":"components/Block.js","original":{"line":43,"column":14}},{"generated":{"line":109,"column":23},"source":"components/Block.js","original":{"line":41,"column":12}},{"generated":{"line":109,"column":24},"source":"components/Block.js","original":{"line":40,"column":0}},{"generated":{"line":110,"column":0},"source":"components/Block.js","original":{"line":53,"column":1}},{"generated":{"line":111,"column":0},"source":"components/Block.js","original":{"line":54,"column":1}},{"name":"Component","generated":{"line":115,"column":2},"source":"components/Block.js","original":{"line":6,"column":20}},{"name":"Block","generated":{"line":117,"column":15},"source":"components/Block.js","original":{"line":71,"column":15}}],"sources":{"components/Block.js":"import React,{Component} from 'react'\nimport {Button} from 'react-bootstrap'\nimport Transaction from './Transaction.js'\n\n\nclass Block extends Component{\n    state={displayTransaction:false}\n    toggleTransaction=()=>{\n        this.setState({displayTransaction:!this.state.displayTransaction})\n    }\nget displayTransactions(){\n    const {data}=this.props.block\n    const stringifiedData=JSON.stringify(data)\n\nconst dataDisplay=stringifiedData.length>35?`${stringifiedData.substring(0,35)}...`:stringifiedData\n\nif(this.state.displayTransaction){\nreturn(\n    <div>\n      {\n          data.map(transaction=>(\n            <div key={transaction.id}>\n              <hr />\n              <Transaction transaction={transaction} />\n              </div>\n          ))\n       \n         }   <br />\n        <Button\n            bsstyle=\"danger\"\n            bss ize=\"small\"\n            onClick={this.toggleTransaction}\n          >\n            Show Less\n          </Button>  \n      </div>\n)\n}\nelse{\nreturn (\n            <div>\n              <div>Data: {dataDisplay}</div>\n              <Button\n                bsstyle=\"danger\"\n                bssize=\"small\"\n                onClick={this.toggleTransaction}\n              >\n                Show More\n              </Button>\n            </div>\n          \n    )\n}\n}\n\nrender(){\n    \n    const {timestamp,hash}=this.props.block\n    const hashDisplay=`${hash.substring(0,15)}...`\n    \n    \n    return(\n        <div className=\"block\">\n            <div>timestamp:{new Date(timestamp).toLocaleString()}</div>\n            <div>Hash:{hashDisplay}</div>\n            {this.displayTransactions}\n           </div> \n    )\n}\n}\nexport default Block"},"lineCount":null}},"error":null,"hash":"4a06f02137c296cce6dc28ec09e0529d","cacheData":{"env":{}}}